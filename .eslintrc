{
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jquery": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": false
        }
    },
    "extends": "airbnb-base",
    "parser": "babel-eslint",
    "rules": {
        "linebreak-style": ["error", "windows"],
        "no-undef": 0,
        "no-plusplus": 0,
        "brace-style": 0
    }
}
